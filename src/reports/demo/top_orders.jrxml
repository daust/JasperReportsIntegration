	<query language="sql"><![CDATA[SELECT
  o.order_id,
  o.order_timestamp,
  o.order_total,
  c.cust_last_name
  || ', '
  || c.cust_first_name customer_name
FROM
  demo_orders o,
  demo_customers c
WHERE
  o.customer_id = c.customer_id
ORDER BY
  o.order_total DESC]]></query>
